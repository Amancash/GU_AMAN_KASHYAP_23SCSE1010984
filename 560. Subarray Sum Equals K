class Solution {
    public int subarraySum(int[] arr, int k) {
        int n = arr.length;
        int count =0;
        int sum =0;
        for (int i = 0; i < n; i++) {
            String subarray = "";
            for (int j = i; j < n; j++) {
                sum = sum + arr[j];
                
                if(sum == k){
                    count++;     
                    // sum =0;}
                }
            }sum =0;
        }
        return count;
    }
}
