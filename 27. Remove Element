import java.util.ArrayList;
class Solution {
    public int removeElement(int[] arr1, int k) {
        int count =0;

        for(int i =0;i<arr1.length;i++){

            if(arr1[i]!=k){
                arr1[count]= arr1[i];
                count = count +1;
            }
        }
        return count;

    }
}
